  "type": "commonjs",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@aws-sdk/client-s3": "^3.859.0",
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "^26.1.0",
    "leaflet": "^1.9.4",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  }
